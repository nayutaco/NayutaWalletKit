  width: 90,
  height: 90,
  left: 10,
  title: "wewew",
  titleColor: Alloy.Globals.mainColor,
  font: {
    fontSize: 30
  },
  color: Alloy.Globals.mainColor
}
".buttonRow" : {
  layout: "horizontal",
  width: Ti.UI.SIZE,
  height: Ti.UI.SIZE,
  top: 10
}
".buttons" : {
  layout: "vertical",
  width: Ti.UI.SIZE,
  height: Ti.UI.SIZE,
  top:15
}
"#c" : {
  borderWidth: 0,
  font: {
    fontSize: 18
  },
  width: 70
}
"#e" : {
  borderWidth: 0,
  font: {
    fontSize: 18
  },
  width: 70,
